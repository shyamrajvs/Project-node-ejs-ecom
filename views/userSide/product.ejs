    <script>
      function addToWishlist(productId, event) {
          event.preventDefault();
          $.ajax({
              method: 'POST',
              url: '/addToWishlist',
              data: { productId },
              success: function(res) {
                  if (res.addedToWishlist) {
                      const wishlistText = document.getElementById(`wishlist-text-${productId}`);
                      wishlistText.innerHTML = 'Remove from Wishlist';
  
                      Swal.fire({
                          position: "bottom-end",
                          title: "<i class='fa-solid fa-thumbs-up'>&nbsp</i> Item Added To Wishlist",
                          showConfirmButton: false,
                          timer: 1500,
                          customClass: {
                              title: 'swal-title-size',
                              popup: 'swal-bg-color',
                          },
                          html: true,
                          backdrop: false
                      });
                  } else if (res.removedFromWishlist) {
                      const wishlistText = document.getElementById(`wishlist-text-${productId}`);
                      wishlistText.innerHTML = 'Add to Wishlist';
  
                      Swal.fire({
                          position: "bottom-end",
                          title: "<i class='fa-solid fa-thumbs-up'>&nbsp</i> Item Removed From Wishlist",
                          showConfirmButton: false,
                          timer: 1500,
                          customClass: {
                              title: 'swal-title-size',
                              popup: 'swal-bg-color',
                          },
                          html: true,
                          backdrop: false
                      });
                  } else {
                      Swal.fire({
                          position: "bottom-end",
                          title: "Item Not Added To Wishlist",
                          showConfirmButton: false,
                          timer: 1500
                      });
                  }
              },
              error: function(error) {
                  console.error('Error toggling wishlist:', error);
                  window.location.href = '/error'; 
              }
          });
      }
    </script>

    
